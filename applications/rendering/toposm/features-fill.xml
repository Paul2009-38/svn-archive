<Layer name="highway" status="on">
    <StyleName>highway</StyleName>
    <Datasource>
	  &dbsettings;
	  &extents;
      <Parameter name="table">
      	(select way, highway, bridge, tunnel, bicycle from us_osm_line
      	where highway is not null
      	order by z_order) as lines
      </Parameter>
    </Datasource>
</Layer>

<Layer name="railway" status="on">
    <StyleName>railway</StyleName>
    <Datasource>
	  &dbsettings;
	  &extents;
      <Parameter name="table">
      	(select way, railway, bridge, tunnel from us_osm_line
      	where railway is not null
      	order by z_order) as lines
      </Parameter>
    </Datasource>
</Layer>

<Layer name="aeroway" status="on">
    <StyleName>aeroway</StyleName>
    <Datasource>
	  &dbsettings;
	  &extents;
      <Parameter name="table">
      	(select way, aeroway from us_osm_line
      	where aeroway is not null
      	order by z_order) as lines
      </Parameter>
    </Datasource>
</Layer>

