<Style name="fixme-lines">
  <Rule>
    <Filter>[fixme] = 'resurvey'</Filter>
    &maxscale_zoom13;
    <LineSymbolizer>
      <CssParameter name="stroke">#D0006F</CssParameter>
      <CssParameter name="stroke-width">0.75</CssParameter>
      <CssParameter name="stroke-dasharray">4,4</CssParameter>
    </LineSymbolizer>
  </Rule>
</Style>
<Layer name="fixme-lines" status="on" srs="&osm2pgsql_projection;">
    <StyleName>fixme-lines</StyleName>
    <Datasource>
      <Parameter name="table">
      (select way,fixme
       from &prefix;_line
       where fixme in ('resurvey','stub')
      ) as fixme
      </Parameter>
      &datasource-settings;
    </Datasource>
</Layer>

