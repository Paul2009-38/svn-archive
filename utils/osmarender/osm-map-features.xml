<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="osmarender.xsl"?>

<!-- Rules for OSM Map Features 0.3 -->
<!-- This file should be used with Osmarender 2.0 -->
<!-- This file implements a sub-set of the items described at
 http://wiki.openstreetmap.org/index.php/Map_Features -->

<rules 
  xmlns:xlink="http://www.w3.org/1999/xlink"
  xmlns:svg="http://www.w3.org/2000/svg"
  data="data.osm"
  scale="1"
  projection="1.6">
  
  <!-- draw highway shading next -->
  <rule k="highway" v="residential">
    <line class='abutters-residential' /> 
  </rule>
  <rule k="abutters" v="residential">
    <line class='abutters-residential'/> 
  </rule>
  <rule k="abutters" v="retail">
    <line class='abutters-retail'/> 
  </rule>
  <rule k="abutters" v="industrial">
    <line class='abutters-industrial'/> 
  </rule>
  <rule k="abutters" v="commercial">
    <line class='abutters-commercial'/> 
  </rule>
  <rule k="abutters" v="mixed">
    <line class='abutters-mixed'/> 
  </rule>
  

  <!-- For debugging this rule draws a one pixel wide trace
       of *all* segments.  This enables segments that have no
       tags to be identified. Comment it out to hide the debug trace. --> 
  <!--  <rule e="segment" k="*" v="*">  
    <line class='debug'/>
  </rule>
  -->

  <!-- Re-draw natural feature outlines over debug lines -->
  <!--<rule e="way|area" k="natural" v="water">
    <line class='natural-water' /> 
  </rule>
  <rule e="way|area" k="natural" v="land">
    <line class='natural-land' /> 
  </rule>-->

  <!-- draw waterway casings -->
  <rule e="segment|way" k="waterway" v="river">
    <line class='waterway-river-casing'/> 
  </rule>
  <rule e="segment|way" k="waterway" v="canal">
    <line class='waterway-canal-casing'/> 
  </rule>
  <rule e="segment|way" k="waterway" v="drain">
    <line class='waterway-drain-casing'/> 
  </rule>

  <!-- Draw natural features -->
  <rule e="way|area" k="natural" v="water">
    <area class='natural-water' /> 
  </rule>
  <rule e="way|area" k="natural" v="land">
    <area class='natural-land' /> 
  </rule>


  <!-- Draw man-made areas -->
  <rule e="way|area" k="leisure" v="park">
    <area class='leisure-park'/> 
  </rule>
  <rule e="way|area" k="amenity" v="parking">
    <area class='amenity-parking'/> 
  </rule>


  <!-- draw waterway cores -->
  <rule e="segment|way" k="waterway" v="river">
    <line class='waterway-river-core'/> 
  </rule>
  <rule e="segment|way" k="waterway" v="canal">
    <line class='waterway-canal-core'/> 
  </rule>
  <rule e="segment|way" k="waterway" v="drain">
    <line class='waterway-drain-core'/> 
  </rule>


  <!-- next draw paths -->
  <rule e="segment|way" k="highway" v="footway|steps">
    <line class='highway-footway' /> 
    <text k="ref" class='highway-footway-ref' dx='2' dy='-2' />    
  </rule>

  <rule e="segment|way" k="highway" v="steps">
    <line class='highway-steps' /> 
  </rule>

  <rule e="segment|way" k="highway" v="cycleway">
    <line class='highway-cycleway' /> 
    <text k="ref" class='highway-cycleway-ref' dx='2' dy='-2' />
  </rule>

  <rule e="segment|way" k="highway" v="bridleway">
    <line class='highway-bridleway' /> 
    <text k="ref" class='highway-bridleway-ref' dx='2' dy='-2' />
  </rule>


  <!-- draw highway casings for minor roads -->
  <rule e="segment|way" k="highway" v="unclassified|residential">
    <line class='highway-unclassified-casing' />
  </rule>


  <!-- draw highway cores for minor roads -->
  <rule e="segment|way" k="highway" v="unclassified|residential">
    <rule k="oneway" v="~">
      <line class='highway-unclassified-core' />  
    </rule>
    <rule k="oneway" v="1|yes|true">
      <line class='highway-unclassified-core oneway' />  
    </rule>
    <rule k="oneway" v="-1">
      <line class='highway-unclassified-core otherway' />    
    </rule>    
  </rule>
  

  <!-- draw railway lines -->
  <rule e="segment|way" k="railway" v="rail">
    <line class='railway-rail' />
  </rule>


  <!-- draw highway casings for major roads -->
  <rule e="segment|way" k="highway" v="secondary">
      <line class='highway-secondary-casing' />
  </rule>
  <rule e="segment|way" k="highway" v="primary|primary_link">
      <line class='highway-primary-casing' />
  </rule>
  <rule e="segment|way" k="highway" v="trunk|trunk_link">
      <line class='highway-trunk-casing' />
  </rule>
  <rule e="segment|way" k="highway" v="motorway|motorway_link">
      <line class='highway-motorway-casing' />
  </rule>


  <!-- draw highway cores for major roads -->
  <rule e="segment|way" k="highway" v="secondary">
    <rule k="oneway" v="~">
      <line class='highway-secondary-core' />  
    </rule>
    <rule k="oneway" v="1|yes|true">
      <line class='highway-secondary-core oneway' />  
    </rule>    
    <rule k="oneway" v="-1">
      <line class='highway-secondary-core otherway' />    
    </rule>    
  </rule>

  <rule e="segment|way" k="highway" v="primary|primary_link">
    <rule k="oneway" v="~">
      <line class='highway-primary-core' />
    </rule>
    <rule k="oneway" v="1|yes|true">
      <line class='highway-primary-core oneway' />  
    </rule>
    <rule k="oneway" v="-1">
      <line class='highway-primary-core otherway' />    
    </rule>    
  </rule>

  <rule e="segment|way" k="highway" v="trunk|trunk_link">
    <rule k="oneway" v="~">
      <line class='highway-trunk-core' />
    </rule>
    <rule k="oneway" v="1|yes|true">
      <line class='highway-trunk-core oneway' />  
    </rule>
    <rule k="oneway" v="-1">
      <line class='highway-trunk-core otherway' />    
    </rule>    
  </rule>

  <rule e="segment|way" k="highway" v="motorway|motorway_link">
    <rule k="oneway" v="~">
      <line class='highway-motorway-core' />
    </rule>
    <rule k="oneway" v="1|yes|true">
      <line class='highway-motorway-core oneway' />  
    </rule>
    <rule k="oneway" v="-1">
      <line class='highway-motorway-core otherway' />    
    </rule>    
  </rule>


  <!-- draw non-pysical routes -->
  <rule e="segment|way" k="route" v="ferry">
    <line class='route-ferry' />
  </rule>
  

  <!-- draw nodes -->
  <rule e="node" k="place" v="continent">
    <text k="name" class='continent-caption' />
  </rule>

  <rule e="node" k="place" v="country">
    <text k="name" class='country-caption' />
  </rule>

  <rule e="node" k="place" v="state">
    <text k="name" class='state-caption' />
  </rule>

  <rule e="node" k="place" v="region">
    <text k="name" class='region-caption' />
  </rule>

  <rule e="node" k="place" v="county">
    <text k="name" class='county-caption' />
  </rule>

  <rule e="node" k="place" v="city">
    <text k="name" class='city-caption' />
  </rule>

  <rule e="node" k="place" v="town">
    <text k="name" class='town-caption' />
  </rule>

  <rule e="node" k="place" v="village">
    <text k="name" class='village-caption' />
  </rule>

  <rule e="node" k="place" v="hamlet">
    <text k='name' class='hamlet-caption' />
  </rule>

  <rule e="node" k="railway" v="station">
    <circle r="1.5" class="railway-station" />
    <text k="name" class='railway-station-caption' dx='2.5' dy='1.5' />
  </rule>

  <rule e="node" k="amenity" v="place_of_worship">
    <symbol xlink:href="#church" width='2.5' height='5' transform='translate(-1.25,-2.5)' />
  </rule>

  <rule e="node" k="amenity" v="petrolStation">
    <symbol xlink:href="#petrolStation" width='2.5' height='5' transform='translate(-1.25,-3.5)' />
  </rule>

  <rule e="node" k="leisure" v="golf_course">
    <symbol xlink:href="#golfCourse" width='5' height='10' transform='translate(-2.5,-5)' />
  </rule>


  <!-- Draw street names for all highways -->
  <rule e="segment|way" k="highway" v="unclassified|residential">
    <text k="name" text-anchor='middle' startOffset='50%' class="highway-unclassified-name" />
  </rule>

  <rule e="segment|way" k="highway" v="secondary">
    <text k="name" text-anchor='middle' startOffset='50%' class="highway-secondary-name" />
  </rule>

  <rule e="segment|way" k="highway" v="primary|primary_link">
    <text k="name" text-anchor='middle' startOffset='50%' class="highway-primary-name" />
  </rule>

  <rule e="segment|way" k="highway" v="trunk|trunk_link">
    <text k="name" text-anchor='middle' startOffset='50%' class="highway-trunk-name" />
  </rule>

  <rule e="segment|way" k="highway" v="motorway|motorway_link">
    <text k="name" text-anchor='middle' startOffset='50%' class="highway-motorway-name" />
  </rule>


  <!-- Draw refs for all highways -->
  <rule e="node" k="highway" v="unclassified|residential">
    <text k="ref" class='highway-unclassified-ref' dx='2.5' dy='-2.5' />
  </rule>

  <rule e="node" k="highway" v="secondary">
    <text k="ref" class='highway-secondary-ref' dx='2.5' dy='-2.5' />
  </rule>

  <rule e="node" k="highway" v="primary|primary_link">
    <text k="ref" class='highway-primary-ref' dx='2.5' dy='-2.5' />
  </rule>

  <rule e="node" k="highway" v="trunk|trunk_link">
    <text k="ref" class='highway-trunk-ref' dx='2.5' dy='-2.5' />
  </rule>

  <rule e="node" k="highway" v="motorway|motorway_link">
    <text k="ref" class='highway-motorway-ref' dx='2.5' dy='-2.5' />
  </rule>

  <rule e="node" k="highway" v="gate">
    <symbol xlink:href="#gate" width='10' height='5' transform='translate(-5,-2.5)'/>
  </rule>


  <!-- Use the following three rules to display nodes, segments and ways as they would appear in JOSM, overlayed on top of anything else -->
  <!--
  
  <rule e="segment" k="*" v="*">
    <line class='josm-segment' /> 
  </rule>

  <rule e="way" k="*" v="*">
    <line class='josm-way' /> 
  </rule>

  <rule e="node" k="*" v="*">
    <circle r='0.2' class='josm-node' /> 
  </rule>

  -->

  <!-- Use this rule to highlight tags that you want to get rid of, or change -->
  <!--<rule e="way" k="class|highway|waterway|route" v="~">
      <line class='error'/> 
  </rule>-->



  <!-- SVG Definitions - markers, symbols etc go here -->
  <defs>

    <style type="text/css" xmlns="http://www.w3.org/2000/svg">
      .debug {
        stroke-width: 0.1;
        stroke-linecap: round;
        stroke: gray;
        marker-end: url(#segment-direction);
        }
    
      .error {
        stroke-width: 2;
        stroke-linecap: round;
        stroke: red;
        }

      .abutters-residential {
        stroke-width: 9;
        stroke-linecap: round;
        stroke: #f2f2f2;
        fill: none;
        }

      .abutters-retail {
        stroke-width: 9;
        stroke-linecap: round;
        stroke: #ffebeb;
        fill: none;
        }

      .abutters-industrial {
        stroke-width: 9;
        stroke-linecap: round;
        stroke: #ecd8ff;
        fill: none;
        }

      .abutters-commercial {
        stroke-width: 9;
        stroke-linecap: round;
        stroke: #fcffc9;
        fill: none;
        }
        
      .abutters-mixed {
        stroke-width: 9;
        stroke-linecap: round;
        stroke: #d8feff;
        fill: none;
        }

      /* Highways */
      .highway-motorway-casing {
        stroke-width: 2.5; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #000000;
        fill: none;
        }

      .highway-motorway-core {
        stroke-width: 1.5; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #809BC0;
        fill: none;
        }

      .highway-motorway-name {
        fill: black;
        font-family: verdana;
        font-size: 1.5;
        font-weight: normal;
        baseline-shift: -35%;
        }

      .highway-motorway-ref {
        fill: black;
        stroke: white;
        stroke-width: .4;
        font-family: verdana;
        font-size: 7;
        font-weight: bolder;
        }      

      .highway-trunk-casing {
        stroke-width: 2.5; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #000000;
        fill: none;
        }

      .highway-trunk-core {
        stroke-width: 1.5; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #e46d71;
        fill: none;
        }

      .highway-trunk-name {
        fill: black;
        font-family: verdana;
        font-size: 1.5;
        font-weight: normal;
        baseline-shift: -35%;
        }

      .highway-trunk-ref {
        fill: black;
        stroke: white;
        stroke-width: 0.4;
        font-family: verdana;
        font-size: 6;
        font-weight: bolder;
        }      

      .highway-primary-casing {
        stroke-width: 2; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #000000;
        fill: none;
        }

      .highway-primary-core {
        stroke-width: 1; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #7FC97F;
        fill: none;
        }

      .highway-primary-name {
        fill: black;
        font-family: verdana;
        font-size: 1;
        font-weight: bolder;
        stroke: #ffffff;
        stroke-width: 0; 
        baseline-shift: -35%;
        }

      .highway-primary-ref {
        fill: black;
        stroke: white;
        stroke-width: 0;
        font-family: verdana;
        font-size: 4;
        font-weight: bolder;
        }      

      .highway-secondary-casing {
        stroke-width: 2; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #000000;
        fill: none;
        }

      .highway-secondary-core {
        stroke-width: 1; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #FDBF6F;
        fill: none;
        }

      .highway-secondary-name {
        fill: black;
        font-family: verdana;
        font-size: 1;
        font-weight: bolder;
        baseline-shift: -35%;
        }

      .highway-secondary-ref {
        fill: black;
        stroke: white;
        stroke-width: 0.3;
        font-family: verdana;
        font-size: 5;
        font-weight: bolder;
        }      

      .highway-unclassified-casing {
        stroke-width: 1.5; 
        stroke-linecap: butt; 
        stroke-linejoin: round; 
        fill: none;
        stroke: #000000;
        }

      .highway-unclassified-core {
        stroke-width: 1; 
        stroke-linecap: butt;
        stroke-linejoin: round; 
        stroke: #ffffff;
        fill: none;
        }

      .highway-unclassified-name {
        fill: black;
        font-family: verdana;
        font-size: 1;
        font-weight: bold;
        baseline-shift: -35%;
         }

      .highway-unclassified-ref {
        fill: black;
        stroke: white;
        stroke-width: 0.3;
        font-family: verdana;
        font-size: 4.5;
        font-weight: bolder;
        }      

      .highway-bridleway {
        stroke-width: 1; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #70b971;
        fill: none;
        }

      .highway-bridleway-ref {
        fill: black;
        stroke: white;
        stroke-width: 0.3;
        font-family: verdana;
        font-size: 4;
        font-weight: bolder;
        }      

      .highway-cycleway {
        stroke-width: 1; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #008102;
        fill: none;
        }

      .highway-cycleway-ref {
        fill: black;
        stroke: white;
        stroke-width: 0.3;
        font-family: verdana;
        font-size: 4;
        font-weight: bolder;
        }      

      .highway-footway {
        stroke-width: 0.5; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #d79331;
        fill: none;
        }

      .highway-footway-ref {
        fill: black;
        stroke: white;
        stroke-width: 0.3;
        font-family: verdana;
        font-size: 4;
        font-weight: bolder;
        }      

      .highway-steps {
        stroke-width: 0.5; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #707070;
        stroke-dasharray: 0.1, 0.3;
        fill: none;
        }

      /* Waterways */
      .waterway-river-casing {
        stroke-width: 4;
        stroke-linecap: butt;
        stroke-linejoin: round;
        stroke: #aaaaaa;
        fill: none;
        }
        
      .waterway-river-core {
        stroke-width: 3;
        stroke-linecap: butt;
        stroke-linejoin: round;
        stroke: #89bac6;
        fill: none;
        }

      .waterway-canal-casing {
        stroke-width: 2;
        stroke-linecap: butt;
        stroke-linejoin: round;
        stroke: #aaaaaa;
        fill: none;
        }
        
      .waterway-canal-core {
        stroke-width: 1;
        stroke-linecap: butt;
        stroke-linejoin: round;
        stroke: #89bac6;
        fill: none;
        }
        
      .waterway-drain-casing {
        stroke-width: 1;
        stroke-linecap: butt;
        stroke-linejoin: round;
        stroke: #aaaaaa;
        fill: none;
        }
        
      .waterway-drain-core {
        stroke-width: 0.5;
        stroke-linecap: butt;
        stroke-linejoin: round;
        stroke: #89bac6;
        fill: none;
        }

      .railway-rail {
        stroke-width: 1.5;
        stroke-linecap: butt;
        stroke-linejoin: round;
        stroke: #000000;
        fill: none;
        }
        
      .railway-station {
        fill: red;
        stroke: black;
        stroke-width: 0.5;
      }

      .railway-station-caption {
        fill: black;
        stroke: white;
        stroke-width: 0.2;
        font-family: verdana;
        font-size: 4;
        font-weight: bolder;
      }

      .route-ferry {
        stroke-width: 0.5;
        stroke-linecap: butt;
        stroke-linejoin: round;
        stroke: #777777;
        fill: none;
        }
        
      .point-of-interest {
        fill: red;
        stroke: black;
        stroke-width: 0.5;
      }

      .josm-segment {
        stroke-width: 0.2; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #008000;
        fill: none;
        marker-end: url(#segment-direction);
        }

      .josm-way {
        stroke-width: 0.2; 
        stroke-linecap: butt; 
        stroke-linejoin: round;
        stroke: #000060;
        fill: none;
        }
      
      .josm-node {
        fill: #ff0000;
        stroke: none;
      }


      /* Place names */            
      .continent-caption {
        fill: black;
        stroke: white;
        stroke-width: 0.6;
        font-family: verdana;
        font-size: 12;
        font-weight: bolder;
        }
      .country-caption {
        fill: black;
        stroke: white;
        stroke-width: 0.5;
        font-family: verdana;
        font-size: 11;
        font-weight: bolder;
        }
      .state-caption {
        fill: black;
        stroke: white;
        stroke-width: 0.5;
        font-family: verdana;
        font-size: 10;
        font-weight: bolder;
        }
      .region-caption {
        fill: black;
        stroke: white;
        stroke-width: 0.4;
        font-family: verdana;
        font-size: 9;
        font-weight: bolder;
        }
      .county-caption {
        fill: black;
        stroke: white;
        stroke-width: 0.4;
        font-family: verdana;
        font-size: 8;
        font-weight: bolder;
        }
      .city-caption {
        fill: black;
        stroke: white;
        stroke-width: 0.4;
        font-family: verdana;
        font-size: 7;
        font-weight: bolder;
        }
      .town-caption {
        fill: black;
        stroke: white;
        stroke-width: 0.4;
        font-family: verdana;
        font-size: 10;
        font-weight: bolder;
        }
      .village-caption {
        fill: black;
        stroke: white;
        stroke-width: 0.3;
        font-family: verdana;
        font-size: 5;
        font-weight: bolder;
        }
      .hamlet-caption {
        fill: black;
        stroke: white;
        stroke-width: 0.2;
        font-family: verdana;
        font-size: 4;
        font-weight: bolder;
        }
      .church-caption {
        fill: black;
        stroke: white;
        stroke-width: 0.3;
        font-family: verdana;
        font-size: 5;
        font-weight: bold;
        }
      .natural-water {
        fill: #89bac6;
        stroke: #aaaaaa;
        stroke-width: 0.5;
        }
      .natural-land {
        fill: #eeeeee;
        stroke: #e0e0e0;
        stroke-width: 0.1;
        }
      .leisure-park {
        fill: #bde3cb;
        stroke: #6fc18e;
        stroke-width: 0.2;
        }
      .amenity-parking {
        fill: #f7efb7;
        stroke: #e9dd72;
        stroke-width: 0.2;
        }

      .oneway {
        marker-end: url(#triangle);
        }

      .otherway {
        marker-start: url(#invertedTriangle);
        }
    </style>
  
    <marker 
      id="triangle"
      viewBox="0 0 10 10"
      refX="10" refY="5" 
      markerUnits="userSpaceOnUse"
      fill='#a2aee9'
      stroke-width='1'
      stroke='#000000'
      markerWidth="1"
      markerHeight="1"
      orient="auto">
      <path d="M 0,4 L 6,4 L 6,2 L 10,5 L 6,8 L 6,6 L 0,6 z" />
    </marker>

    <marker 
      id="invertedTriangle"
      viewBox="0 0 10 10"
      refX="0" refY="5" 
      markerUnits="userSpaceOnUse"
      fill='#a2aee9'
      stroke-width='1'
      stroke='#000000'
      markerWidth="1"
      markerHeight="1"
      orient="auto">
      <path d="M 10,4 L 4,4 L 4,2 L 0,5 L 4,8 L 4,6 L 10,6 z" />
    </marker>  

    <marker 
